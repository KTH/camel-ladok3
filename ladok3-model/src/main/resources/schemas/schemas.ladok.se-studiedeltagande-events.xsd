      <xs:extension base="sd:AterkalladregistreringEvent">
        <xs:sequence/>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>

  <xs:complexType name="AterkalladregistreringEvent">
<xs:annotation>
<xs:documentation><![CDATA[Händelsen publiceras när:
