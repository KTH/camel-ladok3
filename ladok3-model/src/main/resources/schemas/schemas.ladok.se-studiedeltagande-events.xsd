              <xs:sequence>
                <xs:element maxOccurs="unbounded" minOccurs="0" name="Perioddeltaganden" type="sd:DeltagandePeriod"/>
              </xs:sequence>
            </xs:complexType>
          </xs:element>
          <xs:element name="Senaredelmarkering" type="xs:boolean"/>
          <xs:element name="StudentUID" type="xs:string"/>
